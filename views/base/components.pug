mixin subscribeForm
    <!-- Begin MailChimp Signup Form -->

    <div id="mc_embed_signup">
    <form action="https://varunest.us16.list-manage.com/subscribe/post?u=5f9b1a3bf933355d668248d2e&amp;id=0a1d76497e" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
    <div id="mc_embed_signup_scroll">

    <input type="email" value="" name="EMAIL" class="email" id="mce-EMAIL" placeholder="YOUR EMAIL" required>
    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
    <div class="clear"><input type="submit" value="Subscribe" name="subscribe" id="mc-embedded-subscribe" class="button"></div>
    </div>
    </form>
    </div>

    <!--End mc_embed_signup-->